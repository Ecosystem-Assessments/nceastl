@article{mcclanahan2014a,
  title = {Coral Reefs in a Crystal Ball: Predicting the Future from the Vulnerability of Corals and Reef Fishes to Multiple Stressors},
  shorttitle = {Coral Reefs in a Crystal Ball},
  author = {McClanahan, Timothy R and Graham, Nicholas AJ and Darling, Emily S},
  year = {2014},
  month = apr,
  journal = {Current Opinion in Environmental Sustainability},
  series = {Environmental Change Issues},
  volume = {7},
  pages = {59--64},
  issn = {1877-3435},
  doi = {10.1016/j.cosust.2013.11.028},
  urldate = {2019-11-18},
  abstract = {Coral reef communities are likely to change in the future as reef organisms respond differently to various stressors. In order to predict future reef compositions, we apply estimates of coral and fish vulnerability to two key stressors; climate change (bleaching) and fishing. Most corals were vulnerable to one or both stressors, and future coral communities are likely to comprise stress-tolerant and weedy life histories. Fish assemblages displayed a negative relationship to the two stressors; climate impacts and fishing independently influenced a different suite of species. Most functionally important groups of fishes were more vulnerable to fishing than climate change. While climate change will negatively affect many taxa (particularly reef corals), reducing local fishing impacts should benefit reef futures by enhancing key ecosystem processes.},
  langid = {english}
}

@article{mcdonald-madden2016,
  title = {Using Food-Web Theory to Conserve Ecosystems},
  author = {{McDonald-Madden}, E. and Sabbadin, R. and Game, E. T. and Baxter, P. W. J. and Chades, I. and Possingham, H. P.},
