@article{morissette2009,
  title = {Contrasting Changes between the Northern and Southern {{Gulf}} of {{St}}. {{Lawrence}} Ecosystems Associated with the Collapse of Groundfish Stocks},
  author = {Morissette, Lyne and Castonguay, Martin and Savenkoff, Claude and Swain, Douglas P. and Chabot, Denis and Bourdages, Hugo and Hammill, Mike O. and Mark Hanson, J.},
  year = {2009},
  month = oct,
  journal = {Deep Sea Research Part II: Topical Studies in Oceanography},
  series = {The {{Proceedings}} of the {{ECONORTH Symposium}} on {{Ecosystem Dynamics}} in the {{Norwegian Sea}} and {{Barents Sea}}},
  volume = {56},
