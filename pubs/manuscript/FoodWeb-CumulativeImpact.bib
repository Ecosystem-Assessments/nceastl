@article{burns2021,
  title = {Revealing the Relationship between Feeding and Growth of Larval Redfish ({{Sebastes}} Sp.) in the {{Gulf}} of {{St}}. {{Lawrence}}},
  author = {Burns, Corinne M and Pepin, Pierre and Plourde, St{\'e}phane and Veillet, Guillaume and Sirois, Pascal and Robert, Dominique},
