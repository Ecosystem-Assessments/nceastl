@article{goldenberg2017,
  title = {Boosted Food Web Productivity through Ocean Acidification Collapses under Warming},
  author = {Goldenberg, Silvan U. and Nagelkerken, Ivan and Ferreira, Camilo M. and Ullah, Hadayet and Connell, Sean D.},
  year = {2017},
